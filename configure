echo $ac_n "checking your OS""... $ac_c" 1>&6
echo "configure:1195: checking your OS" >&5

eval "cv_var_system=`${UNAME}`"
case "$cv_var_system" in
  BSD/OS)
    bsd_version=`${UNAME} -r | cut -d . -f 1`
    case "$bsd_version" in
      2)
        echo "$ac_t""BSD/OS 2! statically linked modules are the only choice" 1>&6
        NEED_DL=0
        DEFAULT_MAKE=static
      ;;
      3)
        echo "$ac_t""BSD/OS 3! stuck with an old OS ..." 1>&6
        MOD_CXX=shlicc
        MOD_LD=shlicc
        MOD_STRIP="${STRIP} -d"
        SHLIB_LD="shlicc -r"
        SHLIB_STRIP=touch
        cat >> confdefs.h <<\EOF
#define MODULES_OK 1
EOF
      ;;
      *)
        echo "$ac_t""BSD/OS 4+! ok I spose" 1>&6
        MOD_LD="${CXX} "
        MOD_STRIP="${STRIP} -d"
        SHLIB_LD="${CXX} -shared -nostartfiles"
        cat >> confdefs.h <<\EOF
#define MODULES_OK 1
EOF
      ;;
    esac
    ;;
  CYGWIN*)
    echo "$ac_t""Cygwin" 1>&6
    NEED_DL=0
    DEFAULT_MAKE=static
    ;;
  HP-UX)
    echo "$ac_t""HP-UX, just shoot yourself now" 1>&6
    HPUX=yes
    MOD_LD="g++ -fPIC -shared"
    SHLIB_CXX="g++ -fPIC"
    SHLIB_LD="ld -b"
    NEED_DL=0
    cat >> confdefs.h <<\EOF
#define MODULES_OK 1
EOF
    cat >> confdefs.h <<\EOF
#define HPUX_HACKS 1
EOF
    if test "x`${UNAME} -r | cut -d . -f 2`" = "x10"
    then
      cat >> confdefs.h <<\EOF
#define HPUX10_HACKS 1
EOF
    fi
    ;;
  dell)
    echo "$ac_t""Dell SVR4" 1>&6
    SHLIB_STRIP=touch
    NEED_DL=0
    MOD_LD="g++ -lelf -lucb"
    ;; 
  IRIX)
    echo "$ac_t""you are cursed with IRIX" 1>&6
    IRIX=yes
    SHLIB_STRIP=touch
    NEED_DL=0
    DEFAULT_MAKE=static
    ;;
  IRIX64)
    echo "$ac_t""IRIX64" 1>&6
    IRIX=yes
    SHLIB_STRIP=strip
    NEED_DL=0
    DEFAULT_MAKE=static
    ;;
  Ultrix)
    echo "$ac_t""Ultrix" 1>&6
    NEED_DL=0
    SHLIB_STRIP=touch
    DEFUALT_MAKE=static
    ;;
  BeOS)
    echo "$ac_t""BeOS" 1>&6
    NEED_DL=0
    SHLIB_STRIP=strip
    DEFUALT_MAKE=static
    ;;
  Linux)
    echo "$ac_t""Linux! The choice of the GNU generation" 1>&6
    LINUX=yes
    MOD_LD="${CXX}"
    SHLIB_LD="${CXX} -shared -nostartfiles"
    cat >> confdefs.h <<\EOF
#define MODULES_OK 1
EOF
    ;;
  Lynx)
    SHLIB_STRIP=touch
    echo "$ac_t""Lynx OS" 1>&6
    ;;
  OSF1)
    echo "$ac_t""OSF..." 1>&6
    case `${UNAME} -r | cut -d . -f 1` in
      V*)
        echo "$ac_t""   Digital OSF" 1>&6
        if test "x$AWK" = "xgawk"
        then
          AWK=awk
        fi
        MOD_CXX=g++
        MOD_LD=g++
        SHLIB_CXX=g++
        SHLIB_LD="ld -msym -S -shared -expect_unresolved \*"
        SHLIB_STRIP=touch
        cat >> confdefs.h <<\EOF
#define MODULES_OK 1
EOF
        ;;
      1.0|1.1|1.2)
        echo "$ac_t""   pre 1.3" 1>&6
        SHLIB_LD="ld -R -export :"
        cat >> confdefs.h <<\EOF
#define MODULES_OK 1
EOF
        cat >> confdefs.h <<\EOF
#define OSF1_HACKS 1
EOF
        ;;
      1.*)
        echo "$ac_t""   1.3+" 1>&6
        SHLIB_CXX="${CXX} -fpic"
        SHLIB_LD="ld -shared"
        cat >> confdefs.h <<\EOF
#define MODULES_OK 1
EOF
        cat >> confdefs.h <<\EOF
#define OSF1_HACKS 1
EOF
        ;;
      *)
        echo "$ac_t""   Some other weird OSF! No modules for you..." 1>&6
        NEED_DL=0
        DEFAULT_MAKE=static
        ;;
    esac
    cat >> confdefs.h <<\EOF
#define STOP_UAC 1
EOF
    ;;
  SunOS)
    if test "x`${UNAME} -r | cut -d . -f 1`" = "x5"
    then
      echo "$ac_t""Solaris -- yay" 1>&6
      SHLIB_LD="/usr/ccs/bin/ld -G -z text"
    else
      echo "$ac_t""SunOS -- sigh" 1>&6
      SUNOS=yes
      SHLIB_LD=ld
      SHLIB_STRIP=touch
      cat >> confdefs.h <<\EOF
#define DLOPEN_1 1
EOF
    fi
    MOD_CXX="${CXX} -fPIC"
    SHLIB_CXX="${CXX} -fPIC"
    cat >> confdefs.h <<\EOF
#define MODULES_OK 1
EOF
    ;;
  *BSD)
    echo "$ac_t""FreeBSD/NetBSD/OpenBSD - choose your poison" 1>&6
    SHLIB_CXX="${CXX} -fpic"
    SHLIB_LD="ld -Bshareable -x"
    cat >> confdefs.h <<\EOF
#define MODULES_OK 1
EOF
    ;;
  *)
    if test -r "/mach"
    then
      echo "$ac_t""NeXT...We are borg, you will be assimilated" 1>&6
      echo "$ac_t""break out the static modules, it's all you'll ever get :P" 1>&6
      NEED_DL=0
      DEFAULT_MAKE=static
      cat >> confdefs.h <<\EOF
#define BORGCUBES 1
EOF
    else
      if test -r "/cmds"
      then
        echo "$ac_t""QNX" 1>&6
        SHLIB_STRIP=touch
        NEED_DL=0
        DEFAULT_MAKE=static
      else
        echo "$ac_t""Something unknown!" 1>&6
        echo "$ac_t""If you get dynamic modules to work, be sure to let the devel team know HOW :)" 1>&6
        NEED_DL=0
        DEFAULT_MAKE=static
      fi
    fi
    ;;
esac


